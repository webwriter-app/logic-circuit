      <trans-unit id="scfcffcf35292bc6f">
        <source>Simulate</source>
        <target>Προσομοίωση</target>
      </trans-unit>
      <trans-unit id="s4635f32ac7e5b41b">
        <source>Drag items here to delete them</source>
        <target>Σύρετε στοιχεία εδώ για να τα διαγράψετε</target>
      </trans-unit>
      <trans-unit id="s6aa6cfde13238a46">
